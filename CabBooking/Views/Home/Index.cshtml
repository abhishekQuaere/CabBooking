@model CabBooking.Models.MainModel
@{
    ViewBag.Title = "Home";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<style>
    .wheel-collection-item img {
        padding-top: 0px;
        display: inline-block;
        margin-left: 0px;
    }

    .wheel-start:after {
        background-color: #f9b931 !important;
    }

    .wheel-btn {
        background-color: #f54108 !important;
    }

    .wheel-start-form label.promo input {
        padding: 18px 15px;
        width: 100%;
        color: #fffbfb;
        line-height: 25px;
        text-transform: uppercase;
        min-height: 60px;
    }
</style>

<div class="wheel-start">
    <img src="images/bg1.jpg" alt="" class="wheel-img">
    <div class="container padd-lr0">
        <div class="col-lg-6">
            <div class="wheel-start-form">
                @*@Html.Partial("_ResponseMessage")*@
                <form action="/Home/Index" method="post">
                    <label for="input-val11">
                        <span>Dropping Off &nbsp; <i class="fa fa-map-marker" aria-hidden="true"></i></span>
                        <input type="text" id="dropping" class="controls" name="Source" placeholder="ZIP, City, Airport or Address" required>
                        <input type="hidden" id="dlat" />
                        <input type="hidden" id="dlng" />
                    </label>
                    <label for="input-val12">
                        <span>Picking Up  &nbsp; <i class="fa fa-map-marker" aria-hidden="true"></i></span>
                        <input type="text" id="picking" class="controls" name="Destination" placeholder="ZIP, City, Airport or Address" required>
                        <input type="hidden" id="plat" />
                        <input type="hidden" id="plng" />
                        <input type="hidden" id="distance" />
                    </label>

                    @*<div class="clearfix">
                            <div class="wheel-date">
